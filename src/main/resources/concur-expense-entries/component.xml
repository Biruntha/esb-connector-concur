<component name="concur-expense-entries" type="synapse/template">
    <subComponents>
        <component name="getExpenseEntry">
            <file>getExpenseEntry.xml</file>
            <description>Returns an expense entry by ID.</description>
        </component>
        <component name="getAllExpenseEntries">
            <file>getAllExpenseEntries.xml</file>
            <description>Returns all expense entries owned by the user.</description>
        </component>
        <component name="deleteExpenseEntry">
            <file>deleteExpenseEntry.xml</file>
            <description>Deletes the specified expense entry.</description>
        </component>
        <component name="createExpenseEntry">
            <file>createExpenseEntry.xml</file>
            <description>Creates a new expense entry.</description>
        </component>
    </subComponents>
</component>